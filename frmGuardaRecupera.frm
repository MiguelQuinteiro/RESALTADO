Private Sub cmdRecuperarProblema_Click()
    If Combo1.Text <> "" Then
        Open "Problemas.RND" For Random As #1 Len = Len(miRegistroProblema)
        miNumeroRegistro = Val(txtRecuperaProblemaNumero)
        miNumeroRegistro = Combo1.Text
        Get #1, miNumeroRegistro, miRegistroProblema
        temporal = ""
        temporal = Trim(miRegistroProblema.Problema)
        For x = 1 To 81
            If Mid(temporal, (x), 1) = "-" Then
                txtMuestra(x) = ""
            Else
                txtMuestra(x) = Mid(temporal, (x), 1)
            End If
        Next x
        Close #1
        ' LIMPIA LOS TEXT BOX DEL MANEJO DE ARCHIVOS
        txtNumeroProblema = ""
        txtRecuperaProblemaNumero = ""
    End If
    'Call cmdCargar_Click
    'txtProceso.Text = "RECUPERADO PROBLEMA N: " + Str(miNumeroRegistro)
End Sub

Private Sub Form_Load()
    Dim x As Integer
    Open "Problemas.RND" For Random As #1 Len = Len(miRegistroProblema)
    For x = 1 To 100
        miNumeroRegistro = x
        Get #1, miNumeroRegistro, miRegistroProblema
        'If miRegistroProblema <> "" Then
            Combo1.AddItem miNumeroRegistro
        'End If
    Next x
    Close #1
End Sub
